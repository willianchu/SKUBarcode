{
  "manifest_version": 3,
  "name": "SKU Barcode Transformer",
  "version": "1.0",
  "description": "Transforms SKUs into barcodes on web pages",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "backgroundScript.js"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
